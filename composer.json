{
    "name": "micro/plugin-doctrine",
    "description": "Micro Framework: Doctrine ORM Supports",
    "license": "MIT",
    "type": "micro-plugin",
    "authors": [
        {
            "name": "Stanislau.Komar",
            "email": "stanislau_komar@epam.com"
        }
    ],
    "require": {
        "doctrine/orm": "^2",
        "micro/kernel": "^2.0",
        "micro/plugin-console": "^2.0",
        "symfony/cache": "^6"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.34"
    },
    "autoload": {
        "psr-4": {
            "Micro\\Plugin\\Doctrine\\": "/"
        },
        "exclude-from-classmap": [
            "/Tests"
        ]
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
